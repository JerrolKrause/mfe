{
  "name": "mfe-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npx nx serve app-shell",
    "start:all": "concurrently \"npx nx serve app-shell\" \"npx nx serve assets\" \"npx nx serve loan-products\"",
    "reinstall": "rmdir node_modules /s /q && rmdir .angular /s /q && del package-lock.json /q &&  npm i",
    "ng": "ng",
    "env": "node environment-load.config.js",
    "prod": "http-server ./dist/apps/app-shell/browser -o -g",
    "prod:static": "http-server ./dist/static -o -g",
    "build:qa": "npx nx build app-shell --configuration=qa",
    "build:uat": "npx nx build app-shell --configuration=uat",
    "build:prod": "npx nx build app-shell --configuration=prod --base-href=/",
    "build:prod:assets": "npx nx build assets --configuration=prod --base-href=/",
    "build:prod:loan-products": "npx nx build loan-products --configuration=prod --base-href=/",
    "build": "ng build",
    "build:stats": "npx nx build app-shell --stats-json && npm run bundle-report",
    "build:source": "npx nx build app-shell --configuration=production --source-map",
    "bundle-report": "webpack-bundle-analyzer dist/apps/app-shell/browser/stats.json -p 8887",
    "docs": "compodoc -p tsconfig.json",
    "docs:serve": "compodoc -p tsconfig.json -s",
    "docs:watch": "compodoc -p tsconfig.json -s -w",
    "models:graph": "node libs/models/src/lib/utils/pre-processing.util.js && graphql-codegen --verbose --debug --config libs/models/src/lib/models-step1.codegen.ts && npx gqlg --schemaFilePath libs/models/src/lib/dist/global.graphql --destDirPath libs/models/src/lib/temp/gqlg/ --depthLimit 2 && graphql-codegen --verbose --debug --config libs/models/src/lib/models-step2.codegen.ts && node libs/models/src/lib/utils/post-processing.util.js && prettier --write libs/models/src/lib/**/*.{ts,tsx,js,jsx,json,scss,css,html,md,gql}",
    "models:rest": "",
    "ssr:prerender": "npx nx app-shell:prerender --routes-file routes.txt",
    "ssr:dev": "ng run app-shell:serve-ssr",
    "ssr:build": "npx nx build app-shell --configuration=production && ng run app-shell:server",
    "ssr:serve": "node dist/server/main.js",
    "deploy": "npx nx build app-shell --base-href=/mfe/ --verbose --configuration=production && angular-cli-ghpages --no-silent --dir=dist/apps/app-shell/browser",
    "deploy:ssr": "ng run mfe-starter:deploy-ssr --routes-file routes.txt && angular-cli-ghpages --no-silent --dir=dist/browser",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.0",
    "@angular/common": "18.2.0",
    "@angular/compiler": "18.2.0",
    "@angular/core": "18.2.0",
    "@angular/forms": "18.2.0",
    "@angular/platform-browser": "18.2.0",
    "@angular/platform-browser-dynamic": "18.2.0",
    "@angular/platform-server": "18.2.0",
    "@angular/router": "18.2.0",
    "@angular/ssr": "18.2.0",
    "@apollo/client": "^3.9.9",
    "@aws/clickstream-web": "^0.12.1",
    "@fortawesome/angular-fontawesome": "0.14.1",
    "@fortawesome/fontawesome-free": "6.5.1",
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-regular-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@graphql-codegen/introspection": "^4.0.3",
    "@ngneat/until-destroy": "^10.0.0",
    "@nx/angular": "19.6.0",
    "@types/react": "^18.2.79",
    "angular-cli-ghpages": "^2.0.0-beta.2",
    "apollo-angular": "^6.0.0",
    "bootstrap": "^5.3.3",
    "express": "~4.18.2",
    "gql-generator": "^2.0.0",
    "graphql": "^16.8.1",
    "primeicons": "^6.0.1",
    "primeng": "17.12.0",
    "ramda": "^0.29.1",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.7.5",
    "tsc": "^2.0.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-architects/module-federation": "^18.0.4",
    "@angular-devkit/build-angular": "18.2.0",
    "@angular-devkit/core": "18.2.0",
    "@angular-devkit/schematics": "18.2.0",
    "@angular-eslint/eslint-plugin": "18.0.1",
    "@angular-eslint/eslint-plugin-template": "18.0.1",
    "@angular-eslint/template-parser": "18.0.1",
    "@angular/cli": "~17.2.0",
    "@angular/compiler-cli": "18.2.0",
    "@angular/language-service": "18.2.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-apollo-angular": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@nx/cypress": "19.6.0",
    "@nx/eslint": "19.6.0",
    "@nx/eslint-plugin": "19.6.0",
    "@nx/jest": "19.6.0",
    "@nx/js": "19.6.0",
    "@nx/web": "19.6.0",
    "@nx/workspace": "19.6.0",
    "@schematics/angular": "18.2.0",
    "@swc-node/register": "1.9.2",
    "@swc/core": "1.5.7",
    "@swc/helpers": "0.5.11",
    "@types/express": "4.17.14",
    "@types/jest": "29.5.12",
    "@types/node": "^18.16.9",
    "@types/ramda": "^0.29.12",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.16.0",
    "@typescript-eslint/utils": "7.16.0",
    "browser-sync": "^3.0.0",
    "concurrently": "^8.2.2",
    "cypress": "13.13.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.13.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-preset-angular": "14.1.0",
    "ngx-build-plus": "^18.0.0",
    "nx": "19.6.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.5.4",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
