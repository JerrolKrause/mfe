{
  "name": "@los/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npx nx serve los --hmr",
    "dashboard": "npx nx serve dashboard --hmr",
    "sandbox": "npx nx serve sandbox --hmr",
    "reinstall": "rmdir node_modules /s /q && rmdir .angular /s /q && del package-lock.json /q &&  npm i",
    "ng": "ng",
    "prod": "http-server ./dist/browser -o -g",
    "prod:static": "http-server ./dist/static -o -g",
    "build:qa": "ng build --configuration=qa",
    "build:uat": "ng build --configuration=uat",
    "build:prod": "npx nx build los --configuration=prod",
    "build": "ng build",
    "build:stats": "ng build --configuration=production --stats-json && npm run bundle-report",
    "build:source": "ng build --configuration=production --source-map",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json -p 8887",
    "docs": "compodoc -p tsconfig.json",
    "docs:serve": "compodoc -p tsconfig.json -s",
    "docs:watch": "compodoc -p tsconfig.json -s -w",
    "generate:models": "graphql-codegen",
    "deploy": "npx nx build los --configuration=prod --configuration=production --base-href /mfe/ && angular-cli-ghpages --no-silent --dir=dist/apps/los/browser",
    "deploy:ssr": "ng run financial-status-check:deploy-ssr --routes-file routes.txt && angular-cli-ghpages --no-silent --dir=dist/browser"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~17.2.0",
    "@angular/common": "~17.2.0",
    "@angular/compiler": "~17.2.0",
    "@angular/core": "~17.2.0",
    "@angular/forms": "~17.2.0",
    "@angular/platform-browser": "~17.2.0",
    "@angular/platform-browser-dynamic": "~17.2.0",
    "@angular/platform-server": "~17.2.0",
    "@angular/router": "~17.2.0",
    "@angular/ssr": "~17.2.0",
    "@apollo/client": "^3.9.9",
    "@fortawesome/angular-fontawesome": "0.14.1",
    "@fortawesome/fontawesome-free": "6.5.1",
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-regular-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@ngneat/until-destroy": "^10.0.0",
    "@nx/angular": "18.1.3",
    "angular-cli-ghpages": "^2.0.0-beta.2",
    "apollo-angular": "^6.0.0",
    "bootstrap": "^5.3.3",
    "express": "~4.18.2",
